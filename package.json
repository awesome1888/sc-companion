{
  "name": "sc-companion",
  "version": "1.0.0",
  "description": "A set of mixins for Styled-Components",
  "module": "build/client.js",
  "scripts": {
    "build": "npm-run-all --parallel build.*.prod",
    "build.client.prod": "webpack --config webpack.client.js --mode production",
    "build.dev": "npm-run-all --parallel build.*.dev",
    "build.client.dev": "webpack --config webpack.client.js --mode development --watch",
    "test.u": "jest -c ./jest/config.unit.js --forceExit"
  },
  "keywords": [],
  "author": "gannochenko.sv@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/awesome1888/sc-companion"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
